package negocio.tienda.imp;

public class TransferPresupuesto
{
	private int departamento;
	private int tienda;
	private double cantidad;
	private boolean activo;
	
	public TransferPresupuesto(int departamento, int tienda)
	{
		this.departamento = departamento;
		this.tienda = tienda;
	}	
	
	public TransferPresupuesto(int departamento, int tienda, boolean activo)
	{
		this.departamento = departamento;
		this.tienda = tienda;
		this.activo = activo;
	}
	
	public TransferPresupuesto(int departamento, int tienda, double cantidad, boolean activo)
	{
		this.departamento = departamento;
		this.tienda = tienda;
		this.cantidad = cantidad;
		this.activo = activo;
	}
	
	public int getDepartamento()
	{
		return departamento;
	}
	
	public int getTienda()
	{
		return tienda;
	}
	
	public Double getCantidad()
	{
		return cantidad;
	}
	
	public boolean getActivo()
	{
		return activo;
	}
	
	public void setDepartamento(int departamento)
	{
		this.departamento = departamento;
	}
	
	public void setTienda(int tienda)
	{
		this.tienda = tienda;
	}
	
	public void setCantidad(Double cantidad)
	{
		this.cantidad = cantidad;
	}	
	
	public void setActivo(Boolean activo)
	{
		this.activo = activo;
	}

	public String toString()
	{
		return "Departamento: " + departamento + " Tienda: " + tienda + " Cantidad: " + cantidad + " Activo: " + activo;
	}
}
